
@import "./var"
* {
    border: 0em;
    padding: 0.4em;
    margin: 0.4em;
    border-radius: 0.3em;
    font-size: base-size px;
    font-family: 'Helvetica Neue', Helvetica, 'PingFang SC', 'Hiragino Sans GB', 'Microsoft YaHei', '微软雅黑', Arial, sans-serif;
    box-shadow: 0em 0em 0.3em 0em rgba(0, 0, 0, 0.2);
}

html {
    margin: 0em;
    padding: 0em;
}

body {
    margin: 0em;
}
div{
    margin: 0.6em;
}
button {
    display: inline-block;
    text-align: center;
}
h1,h2,h3,h4,h5,h6 {
    font-weight: 900;
}